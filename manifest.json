{
  "manifest_version": 3,
  "name": "AgencySMM For Blaze",
  "version": "3.2",
  "description": "a AgencySMM.com demo product",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting",
    "storage",
    "tabs",
    "sidePanel"
  ],
  "host_permissions": [
    "https://*.blaze.ai/*",
    "https://*.cloudinary.com/*"
  ],
  "action": {
    "default_title": "AgencySMM For Blaze"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}